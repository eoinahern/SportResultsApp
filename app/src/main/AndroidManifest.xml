<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.eoinahern.sportsfeedapp"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:name=".MyApplication"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".UI.launcher.LauncherActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".UI.signup.signupSport.SignupSportActivity" />
		<activity android:name=".UI.signup.signupTeams.SignupTeamsActivity" />
		<activity android:name=".UI.livescoreall.LiveScoreAllActivity" />
		<activity android:name=".UI.livescoreindividual.LiveScoreIndividualActivity"></activity>
	</application>

</manifest>